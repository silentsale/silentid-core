{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SilentId": {
    "Environment": "Development",
    "ApiVersion": "v1",
    "ApplicationName": "SilentID API"
  },
  "Jwt": {
    "Issuer": "silentid-api",
    "Audience": "silentid-app",
    "AccessTokenExpiryMinutes": 15,
    "RefreshTokenExpiryDays": 7
  }
}
